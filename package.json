{
    "name": "resume",
    "type": "module",
    "version": "0.0.2",
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro"
    },
    "dependencies": {
        "@fontsource-variable/noto-serif-georgian": "^5.0.8",
        "astro": "^2.10.12",
        "chrono-node": "^2.6.4"
    },
    "devDependencies": {
        "lightningcss": "^1.21.5",
        "prettier": "^3.0.2",
        "prettier-plugin-astro": "^0.11.1",
        "typescript": "^5.1.6"
    },
    "prettier": {
        "tabWidth": 4,
        "arrowParens": "avoid",
        "printWidth": 100,
        "plugins": [
            "./node_modules/prettier-plugin-astro"
        ],
        "overrides": [
            {
                "files": "*.astro",
                "options": {
                    "parser": "astro"
                }
            }
        ]
    }
}
